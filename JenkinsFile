pipeline {
  agent any 
    stages {
      stage('Compile') {
		steps {
          echo "Compile is sucessful"
			}
		}
		stage ('Junit') {
			steps {
				echo "Junit is sucessful"
			}		
		}
		stage ('Build') {
			steps {
				echo "Build is sucessful"
			}		
		}
		stage ('Deploy') {
			steps {
				echo "Deploy is sucessful"
			}		
		}
	}
}
  
